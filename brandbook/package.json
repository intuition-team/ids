{
  "name": "@intuition.team/wds-brandbook",
  "version": "0.1.0",
  "description": "Winvestor Design System Brandbook",
  "repository": "https://github.com/intuition-team/wds.git",
  "scripts": {
    "start": "yarn && yarn clean && parcel serve src/*.pug",
    "prestart": "open-cli http://localhost:1234/index.html",
    "build": "yarn && yarn clean && parcel build src/*.pug",
    "clean": "rimraf static/assets",
    "lint": "yarn eslint && yarn stylelint",
    "eslint": "eslint --ext .jsx,.js src",
    "stylelint": "stylelint \"src/**/*.scss\" --syntax scss",
    "prettier": "prettier \"src/**/*.{js,jsx,scss}\" --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,scss}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "css-loader": "2.0.1",
    "eslint": "5.10.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "3.0.0",
    "husky": "2.3.0",
    "lint-staged": "8.1.7",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.13.1",
    "prettier": "1.15.3",
    "rimraf": "2.6.2",
    "sass": "1.23.7",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "stylelint": "9.9.0",
    "stylelint-config-recommended": "2.1.0",
    "stylelint-scss": "3.4.1",
    "url-loader": "1.1.2"
  },
  "dependencies": {
    "jquery": "3.3.1",
    "opn-cli": "5.0.0",
    "parcel-bundler": "1.12.4",
    "photoswipe": "4.1.3",
    "pug": "2.0.3",
    "tooltipster": "4.2.7"
  }
}
